@{
    ViewBag.Title = "GooglePlaces via JQuery";
}

@section scripts{

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAMrD1y782H_DWSR-Y02QLxGzOAdFbWJP4&libraries=places"></script>
    <script src="~/Scripts/MyCustomScripts/MyJQueryPlayTime.js"></script>
    <script>

        $(document).ready(function () {
            initializeMyJqueryPlayTime();

        });
    </script>

}
<div>
    <h2 class="well">

        <img src="~/Content/powered-by-google/desktop/powered_by_google_on_white_hdpi.png" />
    </h2>
    <hr />
</div>

<div class="form-horizontal row">

    <div class="form-group">

        <input type="button" class="btn btn-primary" id="btnLoadMyMap" value="Click Me To Load My Map" />
    </div>
    <div class="form-group">
        <div class="col-sm-8">
            <div id="divMyMap"></div>
        </div>
        <div id="divMyMapInformation" class="col-sm-4 alert alert-info text-left extraPadding hidden">
            <div class="modal-header">Useful Information About My Map</div>
            <div class="row form-group-sm h6">-Current Position Latitude (Horizontal Lines): <label id="lblCurrentPositionLatitude"></label></div>
            <div class="row form-group-sm h6">-Current Position Longtitude (Vertical Lines): <label id="lblCurrentPositionLongtitude"></label></div>
            <div class="row form-group-sm h6">-Bouncing Red Marker On Map Points To Your Current-Position</div>
            <div class="row form-group-sm h6">-Map Zoom Value: <label id="lblMapZoomValue"></label></div>
            <div class="row form-group-sm h6">-Circle Radius (meters): <label id="lblCircleRadius"></label></div>
        </div>
    </div>
</div>